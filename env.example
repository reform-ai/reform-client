# Basketball Form Analyzer - Environment Variables
# Copy this file to .env and fill in your values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# LLM Configuration
LLM_MODEL=gpt-3.5-turbo
LLM_MAX_TOKENS=150
LLM_TEMPERATURE=0.7

# Audio Configuration
AUDIO_VOLUME=0.8
AUDIO_RATE=1.0
AUDIO_PITCH=1.0

# Camera Configuration
CAMERA_WIDTH=640
CAMERA_HEIGHT=480

# Feedback Configuration
IMMEDIATE_FEEDBACK_MS=1000
AI_FEEDBACK_MS=5000
AGGREGATED_ANALYSIS_MS=30000

# TensorFlow Configuration
TF_INIT_TIMEOUT=10000

# Pose Detection Configuration
POSE_MIN_CONFIDENCE=0.3

# Development Configuration
NODE_ENV=development
ENABLE_LOGGING=true
LOG_LEVEL=info
ENABLE_DEBUG_OVERLAY=true
DEBUG=false
