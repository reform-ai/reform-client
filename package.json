{
  "name": "fitness-ai-form-analyzer",
  "version": "1.0.0",
  "description": "AI-powered fitness form analyzer with real-time pose detection and coaching",
  "main": "index.js",
  "keywords": [
    "fitness",
    "ai",
    "pose-detection",
    "react-native",
    "expo",
    "tensorflow",
    "openai"
  ],
  "author": "FitnessAI Team",
  "license": "MIT",
  "scripts": {
    "start": "expo start",
    "start:go": "expo start --go --port 8083",
    "start:dev": "expo start --dev-client",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean": "rm -rf node_modules && npm install",
    "clean:cache": "expo start --clear",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/cli": "^54.0.6",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@tensorflow-models/pose-detection": "^2.1.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-react-native": "1.0.0",
    "expo": "~54.0.0",
    "expo-camera": "~17.0.7",
    "expo-dev-client": "~6.0.12",
    "expo-gl": "~16.0.7",
    "expo-gl-cpp": "^11.4.0",
    "expo-modules-core": "~3.0.15",
    "expo-sensors": "~15.0.7",
    "expo-speech": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "openai": "^4.104.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-fs": "^2.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.7.0",
    "ora": "^9.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
